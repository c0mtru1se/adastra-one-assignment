    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/react-native": "^11.0.0",
    "@types/react": "~18.0.0",
    "@types/react-native": "~0.69.1",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "nock": "^13.2.9",
    "prettier": "2.7.1",
